{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "no-console": "warn",
    "strict": ["error", "global"],
    "curly": "warn",
    "react/prop-types": "off",
    "no-unused-vars": 1
  }
}